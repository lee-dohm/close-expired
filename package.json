{
  "name": "close-expired",
  "version": "0.0.0",
  "private": true,
  "description": "Accepts a list of issues with dates in their titles, closing any that are past their date",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx ncc build ./src/main.ts",
    "ci": "npm run format-check && npm run lint && npm test",
    "format": "prettier --write **/*.ts **/*.md **/*.yaml **/*.yml",
    "format-check": "prettier --check **/*.ts **/*.md **/*.yaml **/*.yml",
    "lint": "npx eslint src/**/*.ts",
    "start": "npx ncc run ./src/main.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lee-dohm/close-expired.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "@lee-dohm",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/github": "^4.0.0",
    "chrono-node": "^2.1.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.11.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vercel/ncc": "^0.24.1",
    "eslint": "^7.8.1",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.0.2",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.2"
  }
}
